{{#skl_pre_loader}}
<div class="preloader skillab-loader">
  <div class="loader-spinner">
    <svg viewBox="0 0 120 120" version="1.1" xmlns="http://www.w3.org/2000/svg">
      <circle
        class="spinner-load one"
        cx="60"
        cy="60"
        r="20"
        pathLength="1" />
      <circle class="spinner-load two" cx="60" cy="60" r="10" />
      <circle
        class="spinner-load three"
        cx="60"
        cy="60"
        r="30"
        pathLength="1" />
    </svg>
  </div>

</div>

{{#js}}
require(['theme_skilllab/loader/preloader'], function(preloader) {
preloader.init();
});

{{/js}}

{{/skl_pre_loader}}